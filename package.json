{
  "name": "repoprovas-api",
  "version": "0.1.0",
  "description": "This is a repository of tests, it can be extended to your college",
  "main": "dist/server.js",
  "engines": {
    "node": "14.17.5"
  },
  "scripts": {
    "start": "NODE_ENV=prod NODE_TLS_REJECT_UNAUTHORIZED=0 node dist/server.js",
    "build": "tsc",
    "test": "NODE_ENV=test npx jest -i",
    "dev": "ts-node-dev --respawn --transpile-only --inspect -- ./src/server.ts --ignore-watch node_modules",
    "start:dev": "NODE_ENV=dev npx nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node src/server.ts\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "newmigration": "npm run build && npm run typeorm migration:generate -- -n $NAME && npm run build && npm run typeorm migration:run",
    "pre:test:drop": "NODE_ENV=test typeorm schema:drop",
    "pre:test:sync": "NODE_ENV=test typeorm schema:sync"
  },
  "postinstall": "npm run build",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cgabrieu/repoprovas-api.git"
  },
  "keywords": [
    "nodejs",
    "typescript"
  ],
  "author": "cgabrieu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cgabrieu/repoprovas-api/issues"
  },
  "homepage": "https://github.com/cgabrieu/repoprovas-api#readme",
  "dependencies": {
    "aws-sdk": "^2.1046.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "joi": "^17.5.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@types/joi": "^17.2.3",
    "@types/node": "^16.11.14",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^12.1.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.3"
  }
}
